{
  "name": "nadmetrydash",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/)*[^/]+\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|otf)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "autoJobCancelation": true
  }
}